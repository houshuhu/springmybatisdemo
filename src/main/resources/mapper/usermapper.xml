<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.housh.mapper">
    <select id="findbyid"  resultType="com.housh.model.UserModel">
        select Id,Name,OpenId from user where id=#{id}
    </select>

</mapper>