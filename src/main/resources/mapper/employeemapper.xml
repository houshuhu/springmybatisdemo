<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.housh.mapper.EmployeeMapper">
    <select id="getById" resultType="com.housh.model.Employee">
        select Id,last_name,gender,email from tbl_employee where id=#{id}
    </select>

    <select id="getByMapIdAndLastName" parameterType="map" resultType="com.housh.model.Employee">
        select Id,last_name,gender,email from tbl_employee where id=#{id} AND last_name=#{lastname}
    </select>
    <select id="getEmployees" resultType="com.housh.model.Employee">
        select Id,last_name,gender,email from tbl_employee
    </select>

    <select id="getEmpByIdReturnMap" resultType="map">
        select Id,last_name,gender,email from tbl_employee where id=#{id}
    </select>
    <select id="getEmpReturnEmployees" resultType="com.housh.model.Employee">
        select Id,last_name,gender,email from tbl_employee
    </select>
</mapper>